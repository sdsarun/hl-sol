{
  "name": "nest-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:mg:dev": "./node_modules/.bin/sequelize-cli db:migrate --env development",
    "db:mgs:dev": "./node_modules/.bin/sequelize-cli db:migrate:status --env development",
    "db:umg:dev": "./node_modules/.bin/sequelize-cli db:migrate:undo --env development",
    "db:umga:dev": "./node_modules/.bin/sequelize-cli db:migrate:undo:all --env development",
    "db:seed:dev": "./node_modules/.bin/sequelize-cli db:seed:all --env development",
    "db:useed:dev": "./node_modules/.bin/sequelize-cli db:seed:undo:all --env development",
    "db:mg:test": "./node_modules/.bin/sequelize-cli db:migrate --env test",
    "db:mgs:test": "./node_modules/.bin/sequelize-cli db:migrate:status --env test",
    "db:umg:test": "./node_modules/.bin/sequelize-cli db:migrate:undo --env test",
    "db:umga:test": "./node_modules/.bin/sequelize-cli db:migrate:undo:all --env test",
    "db:seed:test": "./node_modules/.bin/sequelize-cli db:seed:all --env test",
    "db:useed:test": "./node_modules/.bin/sequelize-cli db:seed:undo:all --env test",
    "db:gmg": "./node_modules/.bin/sequelize-cli migration:generate --name",
    "db:gs": "./node_modules/.bin/sequelize-cli seed:generate --name",
    "db:gm": "./node_modules/.bin/sequelize-auto"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.8",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.8",
    "@nestjs/platform-express": "^11.0.8",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/swagger": "^11.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^8.0.0",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.8",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.15",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.0",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
